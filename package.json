{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "vite preview",
    "dev": "concurrently -n api,web -c auto \"vercel dev --listen 3000 --yes\" \"vite\"",
    "build": "tsc -b && vite build",
    "build:web": "vite build",
    "vercel-build": "vite build",
    "icons:scaffold": "node scripts/scaffold-icons.mjs",
    "screenshots:scaffold": "node scripts/scaffold-screenshots.mjs",
    "icons:from-logo": "node scripts/icons-from-logo.mjs",
    "screenshots:from-logo": "node scripts/screenshots-from-logo.mjs",
    "assets:from-logo": "npm run icons:from-logo && npm run screenshots:from-logo",
    "android:prep": "npm run build && npx cap copy && npx cap sync",
    "android:open": "npx cap open android",
    "android:dev": "npx cap run android -l --external",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:preview": "npm run build:web && playwright test -c playwright.preview.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "node scripts/run-eslint.mjs . --ext .ts,.tsx --max-warnings 0",
    "lint:docs": "node scripts/run-eslint.mjs --config eslint.docs.config.js \"docs/LiftTrackerAI/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write .",
    "check": "tsc -b && node scripts/run-eslint.mjs . --ext .ts,.tsx --max-warnings=0",
    "typecheck:api": "tsc -p tsconfig.api.json --noEmit",
    "codex:quick": "npm run check && npm run build",
    "codex:fmt": "npm run format && npm run lint",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@capacitor/core": "^7.4.3",
    "@capacitor/haptics": "^7.0.2",
    "@sentry/browser": "^10.10.0",
    "@sentry/react": "^8.32.0",
    "@sentry/vercel-edge": "^8.55.0",
    "@tanstack/react-query": "^5.85.9",
    "@tauri-apps/api": "^2.8.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "fuse.js": "^7.1.0",
    "groq-sdk": "^0.30.0",
    "lucide-react": "^0.541.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "sql.js": "^1.13.0",
    "wouter": "^3.3.5",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "globals": "^15.8.0",
